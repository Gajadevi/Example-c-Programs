#include <stdio.h>

struct item {
    int id;
    char name[20];
};

int main() {
    struct item i[3] = {{1,"Pen"}, {2,"Book"}, {3,"Pencil"}};
    int search;

    scanf("%d", &search);

    for(int j = 0; j < 3; j++) {
        if(i[j].id == search) {
            printf("Item: %s", i[j].name);
            return 0;
        }
    }

    printf("Not Found");
    return 0;
}

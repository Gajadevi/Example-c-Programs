#include <stdio.h>

struct item {
    int id;
    char name[20];
    int qty;
};

int main() {
    struct item i[2];
    int ch;

    for(int j = 0; j < 2; j++) {
        scanf("%d %s %d", &i[j].id, i[j].name, &i[j].qty);
    }

    printf("Inventory List:\n");
    for(int j = 0; j < 2; j++) {
        printf("ID:%d Name:%s Qty:%d\n",
               i[j].id, i[j].name, i[j].qty);
    }
    return 0;
}

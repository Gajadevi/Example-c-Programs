#include <stdio.h>

int main() {
    int n, i, present = 0, status;

    printf("Enter number of students: ");
    scanf("%d", &n);

    for (i = 1; i <= n; i++) {
        printf("Enter attendance of student %d (1=Present, 0=Absent): ", i);
        scanf("%d", &status);
        if (status == 1)
            present++;
    }

    printf("Number of present students = %d", present);

    return 0;
}

#include <stdio.h>

int main() {
    float mark1, mark2, mark3, mark4, mark5, average;

    printf("Enter 5 subject marks:\n");
    scanf("%f %f %f %f %f", &mark1, &mark2, &mark3, &mark4, &mark5);

    average = (mark1 + mark2 + mark3 + mark4 + mark5) / 5;

    printf("Average marks = %.2f", average);

    return 0;
}

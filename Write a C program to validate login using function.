#include <stdio.h>
#include <string.h>

int validate(char u[], char p[]) {
    if (strcmp(u, "admin") == 0 && strcmp(p, "1234") == 0)
        return 1;
    else
        return 0;
}

int main() {
    char user[20], pass[20];

    printf("Enter username: ");
    scanf("%s", user);

    printf("Enter password: ");
    scanf("%s", pass);

    if (validate(user, pass))
        printf("Login successful");
    else
        printf("Login failed");

    return 0;
}

#include <stdio.h>

struct mobile {
    char brand[20];
    char model[20];
    int price;
};

int main() {
    struct mobile m;

    printf("Enter brand: ");
    scanf("%s", m.brand);

    printf("Enter model: ");
    scanf("%s", m.model);

    printf("Enter price: ");
    scanf("%d", &m.price);

    printf("\nMobile Details:\n");
    printf("Brand: %s\nModel: %s\nPrice: %d",
           m.brand, m.model, m.price);

    return 0;
}
